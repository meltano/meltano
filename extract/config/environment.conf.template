[ZUORA]
username   = $ZUORA_USERNAME
password   = $FIXED_ZUORA_PASSWORD
url        = $ZUORA_URL
partner_id = $ZUORA_PARTNER_ID
project_id = $ZUORA_PROJECT_ID

[POSTGRES]

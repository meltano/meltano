roles {
  admin: {
    users: ["admin"]
    acl {
      view {
        designs: ["region"]
        reports: ["*"]
        dashboards: ["*"]
      }
      create {
        reports: ["*"]
      }
      delete {
        reports: ["*"]
        dashboards: ["*"]
      }
    }
  }

  regular {
    users: ["regular"]
    acl: {
      view {
        designs: ["*"]
        reports: ["*"]
        dashboards: ["*"]
      }
    }
  }
}
